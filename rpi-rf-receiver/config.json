{
    "name": "RPi RF Receiver",
    "version": "1.64",
    "slug": "rfreceiver",
    "description": "RPi RF Receiver Script",
    "startup": "application",
    "arch": [
      "aarch64",
      "amd64",
      "armhf",
      "i386"
    ],
    "boot": "auto",
    "devices": ["/dev/mem"],
    "privileged": ["SYS_RAWIO"],
    "gpio": "true",
    "apparmor": "false",
    "ports": {"5055/tcp": null},
    "options": {
      	"code": "/share/rpi-rf_receive.py",
        "requirements": [
            "rpi-rf"
        ],
        "clean": true
    },
    "schema": {
        "code": "str",
	    "requirements": ["str"],
	    "clean": "bool?"
    },
    "map": ["share"]
} 
